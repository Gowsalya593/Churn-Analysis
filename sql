CREATE TABLE powerbi_db.customer_table AS
SELECT 
    CustomerID,
    Subscription,
    `Total Spend`,
    `Contract Length`,
    Churn,
    Tenure
FROM powerbi_db.customer_data;
SELECT COUNT(*) FROM powerbi_db.customer_table;
SELECT COUNT(*) FROM powerbi_db.customer_data;


